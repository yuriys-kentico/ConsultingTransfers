      },
      "backendUri": "https://%AzureStorage%/transfers/{route}",
      "responseOverrides": {
        "response.headers.Strict-Transport-Security": "%Proxy:Strict-Transport-Security%",
        "response.headers.X-Content-Type-Options": "%Proxy:X-Content-Type-Options%",
        "response.headers.X-XSS-Protection": "%Proxy:X-XSS-Protection%",
        "response.headers.X-Frame-Options": "%Proxy:X-Frame-Options%",
        "response.headers.Content-Security-Policy": "%Proxy:Content-Security-Policy%",
        "response.headers.Upgrade-Insecure-Requests": "%Proxy:Upgrade-Insecure-Requests%",
        "response.headers.Referrer-Policy": "%Proxy:Referrer-Policy%",
        "response.headers.Feature-Policy": "%Proxy:Feature-Policy%"
      }
    },
    "ClientProxyDetails": {
      "matchCondition": {
        "route": "/details/{*route}",
        "methods": [ "GET" ]
      },
      "backendUri": "https://%AzureStorage%/details/{route}",
      "responseOverrides": {
        "response.headers.Strict-Transport-Security": "%Proxy:Strict-Transport-Security%",
        "response.headers.X-Content-Type-Options": "%Proxy:X-Content-Type-Options%",
        "response.headers.X-XSS-Protection": "%Proxy:X-XSS-Protection%",
        "response.headers.X-Frame-Options": "%Proxy:X-Frame-Options%",
        "response.headers.Content-Security-Policy": "%Proxy:Content-Security-Policy%",
        "response.headers.Upgrade-Insecure-Requests": "%Proxy:Upgrade-Insecure-Requests%",
        "response.headers.Referrer-Policy": "%Proxy:Referrer-Policy%",
        "response.headers.Feature-Policy": "%Proxy:Feature-Policy%"
      }
    },
    "ClientProxyError": {
      "matchCondition": {
        "route": "/error/{*route}",
        "methods": [ "GET" ]
      },
      "backendUri": "https://%AzureStorage%/error/{route}",
      "responseOverrides": {
        "response.headers.Strict-Transport-Security": "%Proxy:Strict-Transport-Security%",
        "response.headers.X-Content-Type-Options": "%Proxy:X-Content-Type-Options%",
        "response.headers.X-XSS-Protection": "%Proxy:X-XSS-Protection%",
        "response.headers.X-Frame-Options": "%Proxy:X-Frame-Options%",
        "response.headers.Content-Security-Policy": "%Proxy:Content-Security-Policy%",
        "response.headers.Upgrade-Insecure-Requests": "%Proxy:Upgrade-Insecure-Requests%",
        "response.headers.Referrer-Policy": "%Proxy:Referrer-Policy%",
        "response.headers.Feature-Policy": "%Proxy:Feature-Policy%"
      }
    }
  }
}